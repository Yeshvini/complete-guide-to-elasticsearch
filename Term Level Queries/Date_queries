GET /product/_search
{
  "query": {
    "range": {
      "created": {
        "gte": "2010/01/10||-1y"   # subtract one year
      
      }
    }
  }
}

#Round off , add /M either backwat

GET /product/_search
{
  "query": {
    "range": {
      "created": {
        "gte": "2011/05/10-1y/M"
      
      }
    }
  }
}
